{
  "name": "portfolio-with-cms-remix",
  "type": "module",
  "version": "1.0.0",
  "workspaces": [
    "packages/*"
  ],
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "scripts": {
    "lint": "eslint .",
    "prettify": "prettier . --write",
    "verify": "npm run lint && npm run tsc --workspaces",
